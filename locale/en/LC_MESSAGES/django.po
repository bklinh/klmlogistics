msgid "Tư vấn hồ sơ xuất nhập khẩu"
msgstr "Consulting on import and export documents"

msgid "Thủ tục tạm nhập/tái xuất"
msgstr "Temporary import/re-export procedures" 

msgid "Thủ tục xuất nhập khẩu"
